{
	"name": "clau",
	"version": "0.5.3",
	"description": "A quick switcher with fuzzy search across all notes.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"search",
		"switcher",
		"launcher"
	],
	"author": "Ruben Berenguel",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"minisearch": "^7.1.2",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@pixi/filter-glow": "^5.2.1",
		"ml-pca": "^4.1.1",
		"pixi-viewport": "^6.0.3",
		"pixi.js": "^8.12.0",
		"umap-js": "^1.4.0",
		"wink-embeddings-sg-100d": "^1.1.0",
		"wink-eng-lite-web-model": "^1.4.1",
		"wink-nlp": "^2.1.0"
